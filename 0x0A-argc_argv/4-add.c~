#include <stdio.h>
#include <stdlib.h>
/**
 * main -adds positive numbers.
 *
 * Return: 0.
 */
int main(int argc, char **argv)
{
	int i, sum = 0, j;

	for (i = 1; i < argc; i++)
	{
		sum += atoi(argv[i]);
	
	printf("%d\n", i);
	for (j = 0; j <= 9; j++)
	  {

	if (argv[i] != argv[j])
	{
	  (void)argc;
		printf("Error\n");
		return (1);
	}
	  }
	}

	if (argc == 1)
		printf("0\n");

	else if (argc > 1)
		printf("%d\n", sum);
	return (0);
}
